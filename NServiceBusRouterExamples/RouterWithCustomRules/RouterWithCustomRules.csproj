      </Reference>
      <Reference Include="Redactus.Communication.Commands">
        <HintPath>..\NServiceBusRouterExamples.Shared\Dlls\Redactus.Communication.Commands.dll</HintPath>
      </Reference>
      <Reference Include="Redactus.Messaging">
        <HintPath>..\NServiceBusRouterExamples.Shared\Dlls\Redactus.Messaging.dll</HintPath>
      </Reference>
      <Reference Include="Redactus.Wfa.Communication">
        <HintPath>..\NServiceBusRouterExamples.Shared\Dlls\Redactus.Wfa.Communication.dll</HintPath>
      </Reference>
